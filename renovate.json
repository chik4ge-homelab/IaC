{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "reviewers": [
    "chik4ge"
  ],
  "assignees": [
    "chik4ge"
  ],
  "customManagers": [
    {
      "fileMatch": [
        "^variables.tf$"
      ],
      "matchStrings": [
        "variable\\s+\"kubernetes_version\"\\s+{[\\s\\S]*?default\\s+=\\s+\"(?<currentValue>[^\"]+)\""
      ],
      "datasourceTemplate": "kubernetes-api",
      "versioning": "kubernetes-api"
    }
  ]
}
